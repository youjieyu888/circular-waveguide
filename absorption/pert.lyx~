\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{aa}=-\gamma_{1}ch^{2}\rho_{aa}+\gamma_{1}sh{}^{2}\rho_{bb}-\frac{1}{2}\sqrt{\gamma_{1}\gamma_{2}}M(\rho_{ac}+\rho_{ca})$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{bb}=\gamma_{1}(ch^{2}\rho_{aa}-sh^{2}\rho_{bb})+\gamma_{2}(sh^{2}\rho_{cc}-ch^{2}\rho_{bb})+\sqrt{\gamma_{1}\gamma_{2}}M(\rho_{ac}+\rho_{ca})$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{cc}=\gamma_{2}ch^{2}\rho_{bb}-\gamma_{2}sh^{2}\rho_{cc}-\frac{1}{2}\sqrt{\gamma_{1}\gamma_{2}}M(\rho_{ac}+\rho_{ca})$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{ac}+\dot{\rho}_{ca}=-\frac{1}{2}(\gamma_{1}ch^{2}+\gamma_{2}sh^{2})(\rho_{ac}+\rho_{ca})-\sqrt{\gamma_{1}\gamma_{2}}M(\rho_{aa}-2\rho_{bb}+\rho_{cc})$
\end_inset


\end_layout

\begin_layout Standard
perturbation theory
\end_layout

\begin_layout Standard
\begin_inset Formula $\rho_{cc}(0)=1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{aa}(0)=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{bb}(0)=\gamma_{2}sh^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{cc}(0)=-\gamma_{2}sh^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\rho_{aa}=\rho_{aa}(0)+\frac{d\rho_{aa}}{dt}(0)t+\frac{1}{2}\frac{d^{2}\rho_{aa}}{dt^{2}}(0)t^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $=\frac{t^{2}}{2}(\gamma_{1}sh{}^{2}\gamma_{2}sh^{2}-\frac{1}{2}\sqrt{\gamma_{1}\gamma_{2}}M(-\sqrt{\gamma_{1}\gamma_{2}}M))$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $=\frac{t^{2}}{2}\gamma_{1}\gamma_{2}(sh{}^{4}+\frac{1}{2}\gamma_{1}\gamma_{2}M^{2})$
\end_inset


\end_layout

\begin_layout Standard
two level:
\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{aa}=-ch^{2}\rho_{aa}+sh^{2}\rho_{bb}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\dot{\rho}_{bb}=ch^{2}\rho_{aa}-sh^{2}\rho_{bb}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\rho_{ab}+\rho_{ba}=(\frac{1}{2}sh^{2}-\frac{1}{2}ch^{2})(\rho_{ab}+\rho_{ba})+M(\rho_{ab}+\rho_{ba})$
\end_inset


\end_layout

\begin_layout Standard
